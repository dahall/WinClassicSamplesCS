<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="8.0.7" />
    <PackageReference Include="Vanara.Library" Version="4.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Update="ProgressSinkSampleAppRes.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <Target Name="PreBuild" BeforeTargets="PreBuildEvent">
    <Exec Command="call &quot;$(SolutionDir)BuildRC.cmd&quot; ProgressSinkSampleApp ProgressSinkSampleAppRes" IgnoreExitCode="true" />
  </Target>

</Project>