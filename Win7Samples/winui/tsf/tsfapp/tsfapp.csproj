<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net48</TargetFramework>
	<LangVersion>latest</LangVersion>
    <UseWindowsForms>true</UseWindowsForms>
    <DocumentationFile>.\tsfapp.xml</DocumentationFile>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="mainicon.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="mainicon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Vanara.PInvoke.Ole" Version="3.4.9" />
    <PackageReference Include="Vanara.PInvoke.User32" Version="3.4.9" />
    <PackageReference Include="Vanara.PInvoke.TextServicesFramework" Version="3.4.9" />
  </ItemGroup>

</Project>